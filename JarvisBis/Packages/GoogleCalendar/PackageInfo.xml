<?xml version="1.0" encoding="utf-8" ?>
<Package xmlns="http://schemas.myconstellation.io/Constellation/1.8/PackageManifest"
         Name="GoogleCalendar"
         Version="1.0"
         Author="Simon Cordier"
         URL=""
         Description="GoogleCalendar Copyright ©  2017"
         EnableControlHub="false">
  <Compatibility constellationVersion="1.8" dotNetTargetPlatform="net45">
    <Platforms>
      <Platform id="Win32NT" isCompliant="true" />
      <Platform id="Unix" isCompliant="true" />
      <Platform id ="MacOS" isCompliant="false" />
    </Platforms>
  </Compatibility>
  <Settings>
    <Setting name="RefreshInterval" type="Int32" defaultValue="60" description="Refresh every x seconds" />
    <Setting name="CalendarID" type="String" defaultValue="primary" description="Find it in your Calendar parameters : in URL address" />
  </Settings>
</Package>